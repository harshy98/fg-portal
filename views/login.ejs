        <div class="container">
            
            <div class="mt-4 d-flex flex-row">
                    <h4>Login</h4>
                    <a class="ml-auto" href="/">Go to Homepage</a>
                </div>
                <hr>
                <form action="/login" method="POST">
                        <div class="form-group mt-5">
                          <label for="exampleInputEmail1">User ID:</label>
                          <input name="user_id" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="userid" required>
                          <small id="emailHelp" class="form-text text-muted"><em>We'll never share your details with anyone else.</em></small>
                        </div>
                        <div class="form-group">
                          <label for="exampleInputPassword1">Password:</label>
                          <input name="password" type="password" class="form-control" id="exampleInputPassword1" pattern=".{8,16}" placeholder="Password"  required title="8 to 16 characters">
                          <small><a class="hotlinks" href="/forgot">Forgot Password?</a></small>
                        </div>



                        <div id="wrapper2" class="mt-5"><button type="submit" class="btn btn-outline-danger btn-lg">Login</button></div>
                        <div id="wrapper2" class="mt-3">Don't have an account? <a class="hotlinks" href="/register" style="text-indent: 1em"><i class="fas fa-user-plus"></i>Sign up now!</a></div>
                      </form>
        </div>
